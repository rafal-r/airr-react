{
    "name": "airr-react",
    "version": "4.0.2",
    "description": "Reusable React components for building Single Page Apps.",
    "dependencies": {
        "immutability-helper": "^2.9.1"
    },
    "peerDependencies": {
        "react": "^16.8.3"
    },
    "main": "airr-react.js",
    "scripts": {
        "eslint": "eslint lib/*",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "build": "npm run type-check && npm run build:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel lib --out-dir dist --copy-files --extensions \".ts,.tsx\" --source-maps inline",
        "build-webpack": "webpack"
    },
    "keywords": [
        "react",
        "react-components",
        "SPA",
        "navigation",
        "mobile",
        "hybrid",
        "cordova",
        "phonegap"
    ],
    "author": "Rafal Rajtar",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rafal-r/airr-react"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@types/react": "^16.8.12",
        "@typescript-eslint/eslint-plugin": "^1.6.0",
        "@typescript-eslint/parser": "^1.6.0",
        "acorn": "^6.1.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.5",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "prettier": "^1.16.4",
        "react": "^16.8.6",
        "react-docgen-typescript": "^1.12.3",
        "react-dom": "^16.8.6",
        "react-styleguidist": "^9.0.7",
        "source-map-loader": "^0.2.4",
        "typescript": "^3.4.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    },
    "browserslist": [
        ">1%",
        "last 1 version",
        "Firefox ESR",
        "not dead"
    ]
}
